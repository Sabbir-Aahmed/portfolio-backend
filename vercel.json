{
    "builds": [
        {
            "src": "portfolio_backend/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLamdaSize": "15mb", "runtime": "python3.11.3"}
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "portfolio_backend/wsgi.py"
        }
    ]
}